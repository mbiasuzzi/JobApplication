@using System.Data
@model DataTable
@{
    ViewData["Title"] = "Valid Applications";
}

<h1 style="margin-bottom:20px">@ViewData["Title"]</h1>


<table id="validData" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr>
            @foreach (DataColumn col in Model.Columns)
            {
                <th>@col.ColumnName</th>
            }

        </tr>
        </thead>
    <tbody>
        @foreach (DataRow row in Model.Rows)
        {

            <tr>
                @foreach (DataColumn col in Model.Columns)
                {
                    <td>@row[col.ColumnName]</td>
                }
            </tr>
        }
    </tbody>
</table>


@section Scripts{
    <script type="text/javascript">
        $(function () {
            $('#validData').DataTable();
        });
    </script>
}
